<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="metadata" extends="struts-default,json-default">

		<action name="findMetaDataByMapName" class="action.MetaDataAction"
			method="findMetaDataByMapName">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>

		<action name="getReport" class="action.ReportAction" method="getReport">
			<result name="success">template2.jsp</result>
		</action>

		<action name="updateReport" class="action.ReportAction" method="updateReport">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>

	</package>


</struts>    
